{
  "manifest_version": 2,
  "name": "Mise en gras de certaines lettres",
  "version": "1.0",
  "description": "Met en gras les lettres",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_icon": "dyslexie.jpg",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}






  
  
  
  
  



